algoritmo "ex1Start"
var
   nome, sobrenome, nomeCompleto: caractere
inicio
      escreva("Digite seu nome: ")
      leia(nome)
      escreva("Digite seu sobrenome: ")
      leia(sobrenome)
      
      nomeCompleto <- nome + " " + sobrenome
      
      escreval("Seu nome completo é: ", nomeCompleto)
fimalgoritmo