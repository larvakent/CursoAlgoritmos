algoritmo "tabelaVerdade"
var
   i: inteiro
   j: inteiro
inicio
      i <- 10
      j <- 30
      // sim e não = NÃO
      // sim e sim = SIM
      // não e não = NÃO
      // sim ou não = SIM
      // sim ou sim = SIM
      // não ou não = NÃO
      se(i = 10) e (j = 20) entao
         escreval("Fazer algo.")
      senao
         escreval("Fazer nada.")
      fimSe
fimalgoritmo