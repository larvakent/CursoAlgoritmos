algoritmo "ex14start"
var
   i: inteiro
inicio
      i <- 0
      escreval("Início")
      enquanto i <= 100 faca
         escreval(i)
         i <- i + 1
         se(i = 50) entao
            escreval("O i vale 50")
            interrompa
         fimSe
      fimEnquanto
      escreval("Fim")
fimalgoritmo